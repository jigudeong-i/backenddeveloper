<!DOCTYPE html>
<html lang="ko">
	<head>
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<title>Insert title here</title>
	</head>
	<body>
		<script src="../js/jquery-3.7.1.min.js"></script>
		<script>
			$.ajax({
				url: '/firstExample/ajax/json/05.json',
				mehod: 'get',
				dataType: 'json'
			}).done(function (json){
				$("#result").html("");
				let bookInfo = json.books.bookInfo.subject;
				
				for(let i=0; i<bookInfo.length; i++){
					let title = bookInfo[i].title;
					let time = bookInfo[i].price;
					let author = bookInfo[i].author;
					
					let ul = $("<ul>");
					let li1 = $("<li>").html(title);
					let li2 = $("<li>").html(time);
					let li3 = $("<li>").html(teacher);
					
					ul.append(li1).append(li2).append(li3);
					
					$("#result").append(ul);
					$("#result").append("<br />");
				}				
			}).fail(function(xhr, textStatus){
				$("#result").html(textStatus + " (HTTP-" + xhr.status +")");
			});
		</script>
	</body>
</html>